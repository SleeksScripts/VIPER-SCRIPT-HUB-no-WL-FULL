game:GetService("StarterGui"):SetCore("SendNotification",{
    Duration = "2",
    Title = "Loading Script Hub", -- Required
    Text = "You have executed Vipers Script Hub", -- Required
    Icon = "https://www.roblox.com/library/11156630990/BOT-LOGO",
    
})

game.StarterGui:SetCore( "ChatMakeSystemMessage",  { 
    Text = "[VIPERHUB] Thank you for using my script hub",
     Color = Color3.fromRGB( 255,0,0 ), 
     Font = Enum.Font.Arial,
     FontSize = Enum.FontSize.Size24 } )


local UILibrary = loadstring(game:HttpGet("https://pastebin.com/raw/V1ca2q9s"))()

local MainUI = UILibrary.Load("VIPER Script Hub  DISCORD - https://discord.gg/ZjnMetfUDN")
local FirstPage = MainUI.AddPage("All Scripts")

local FirstButton = FirstPage.AddButton("Stone Miner Simulator 2 Script", function()
    if game.PlaceId == 10631181587 then
loadstring(game:HttpGet"https://raw.githubusercontent.com/SleeksScripts/Stone-Miner-Simulator-2-Script/main/Script")()
    else
        game:GetService("StarterGui"):SetCore("SendNotification",{
            Duration = "2",
            Title = "Wrong Game", -- Required
            Text = "This script is for Stone Miner Simulator 2!", -- Required
            
            
        })
    end
end)

